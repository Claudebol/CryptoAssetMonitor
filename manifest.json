{
  "manifest_version": 2,

  "name": "Crypto Asset Monitor v2",
  "description": "This extension will display the USD value of each cryptocurrency provided.\n\nPowered by: cryptocompare.com",
  "version": "2.0",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://sockjs-us2.pusher.com https://stats.pusher.com; object-src 'self'",
  "permissions": [
    "activeTab",
    "cookies",
    "http://ec2-18-188-200-189.us-east-2.compute.amazonaws.com/popup/*",
    "http://localhost:8000/*",
    "https://streamer.cryptocompare.com/",
    "https://stats.pusher.com/*"
  ]

}