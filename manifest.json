{
  "manifest_version": 2,

  "name": "Crypto Asset Monitor v2",
  "description": "This extension will display the USD value of each cryptocurrency provided.\n\nPowered by: cryptocompare.com",
  "version": "2.0",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "http://stsherman.us-east-2.elasticbeanstalk.com/popup/*",
    "http://localhost:8000/*",
    "https://streamer.cryptocompare.com/"
  ]

}